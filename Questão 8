#include <stdio.h>
#include <stdlib.h>

int main()
{
  int vet[] = {4,9,13};
  int i;
  for(i=0;i<3;i++){
  printf("%d ",*(vet+i));
  }

    return 0;
}
// mostra os valore do vetor "vet"

int main(){
  int vet[] = {4,9,13};
  int i;
  for(i=0;i<3;i++){
  printf("%X ",vet+i);
  }
  return 0;
}// mostra o enderÃ§o de memoria ocupado pelas variaveis de vet

