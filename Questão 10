#include <stdio.h>
#include <stdlib.h>

int main()
{
    int x[4];
    char y[4];
    double z[4];
    float w[4];

    int *p;
    char *q;
    double *l;
    float *m;
    p=&x;
    q=&y;
    l=&z;
    m=&w;
    for (int i=0;i<4;i++){
        printf("int %d\n", p+i);

    }printf("\n");
    for (int i=0;i<4;i++){
        printf("char %d\n", q+i);

    }printf("\n");
    for (int i=0;i<4;i++){
        printf("double %d\n", l+i);

    }printf("\n");
    for (int i=0;i<4;i++){
        printf("float %d\n", m+i);

    }printf("\n");
    return 0;
}
// as respostas bateram com o os valores supostos na questão anterior, exeto para o caso do "int" que da um salto de 4 bits e não de 2 como exposto no enunciado da questão.


